{
  "name": "cloud-music",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run": "concurrently pnpm:run:*",
    "save": "concurrently pnpm:save:*",
    "run:dev": "next dev -p 8080",
    "save:build": "next build",
    "start": "next start",
    "lint": "next lint",
    "run:dev:css": "unocss -c uno.config.ts -o styles/uno.css -w pages/**",
    "save:build:css": "unocss -c uno.config.ts -o styles/uno.css pages/**"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.3"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.1.6",
    "@unocss/cli": "^0.50.6",
    "@unocss/preset-attributify": "^0.50.6",
    "@unocss/preset-icons": "^0.50.6",
    "@unocss/preset-uno": "^0.50.6",
    "@unocss/transformer-attributify-jsx": "^0.50.6",
    "@unocss/webpack": "^0.50.6",
    "concurrently": "^8.0.1",
    "unocss": "^0.50.6"
  }
